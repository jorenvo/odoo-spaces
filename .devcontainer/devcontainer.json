{
	"name": "Odoo & Postgres",
	"dockerComposeFile": "docker-compose.yml",
	"service": "odoo",
	"portsAttributes": {
		"8069": {
			"label": "Odoo HTTP",
			"onAutoForward": "notify"
		}
	},
	"remoteUser": "odoo",
	"customizations": {
		"codespaces": {
			"repositories": {
				"jorenvo/enterprise-spaces": {
					"permissions": {
						"contents": "write"
					}
				}
			}
		},
		"vscode": {
			"extensions": [
				"ms-azuretools.vscode-docker"
			]
		}
	},
	"onCreateCommand": "bash .devcontainer/on-create.sh"
}